  "scripts": {
    "app": "node app.js",
    "build": "yarn clean-build && yarn build-typescript",
    "clean-build": "rm -rf build",
    "build-typescript": "tsc -p . --outDir ./build",
    "watch": "yarn clean-build && yarn build-typescript && (yarn watch-typescript)",
    "watch-typescript": "tsc -p . --outDir ./build --watch --pretty",
    "test": "yarn test-backend",
    "test-backend": "mocha build/test/Backend",
    "watch-test-backend": "mocha --watch build/test/Backend",
